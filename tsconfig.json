{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "jsx": "preserve",
    "outDir": "./lib",
    "sourceMap": false,
    "resolveJsonModule": true,
    "lib": ["es6"]
  },
"exclude": ["node_modules","**/dist",]
}
